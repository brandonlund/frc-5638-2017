    public void servoUp(){
    	servo1.set(.5);
    	servo1.setAngle(180);
    	servo2.set(-.5);
    	servo2.setAngle(0);
    }
    public void servoDown(){
    	servo1.set(-.5);
    	servo1.setAngle(0);
    	servo2.set(.5);
    	servo2.setAngle(180);
    }
    public void servoStop(){
    	servo1.set(0);
    	servo2.set(0);
    }
}

